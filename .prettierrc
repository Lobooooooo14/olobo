{
  "trailingComma": "none",
  "tabWidth": 2,
  "semi": false,
  "singleQuote": false,
  "printWidth": 80,
  "endOfLine": "lf",
  "plugins": [
    "@trivago/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss"
  ],
  "importOrder": [
    "^react(.*)",
    "^@ionic(.*)",
    "^@capacitor(.*)",
    "<THIRD_PARTY_MODULES>",
    "^@/hooks(.*)",
    "^@/store(.*)",
    "^@/services(.*)",
    "^@/components(.*)",
    "@/(.*)",
    "^[./]"
  ],
  "importOrderSeparation": true
}
